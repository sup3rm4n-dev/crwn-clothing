{
  "cSpell.words": ["Camo", "Yeezy"],
  "javascript.format.enable": false,
  "terminal.integrated.experimentalRefreshOnResume": true,
  // "terminal.integrated.rendererType": "auto",
  // "terminal.integrated.shell.osx": "/usr/local/bin/bash",
  // "typescript.disableAutomaticTypeAcquisition": true,
  // "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.letterSpacing": 1.7,
  "terminal.integrated.fontSize": 12,
  // "terminal.integrated.fontWeight": "300",
  // "formattingToggle.activateFor": [
  //   "formatOnSave"
  // ],
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "avoid",
  "prettier.jsxBracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 80,
  "prettier.quoteProps": "as-needed",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  // "prettier.useTabs": true,
  "prettier.trailingComma": "es5",
  "html-css-class-completion.includeGlobPattern": "**/*.{css,scss}",
  "eslint.enable": true,
  "formattingToggle.activateFor": ["formatOnSave"],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "node-module-intellisense.scanFileModules": true,
  "node-module-intellisense.autoStripExtensions": [
    ".js",
    ".jsx",
    ".ts",
    ".d.ts",
    ".tsx"
  ],
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importLinebreak": ";\r\n",
  "npm-intellisense.importDeclarationType": "const",
  "npm-intellisense.scanDevDependencies": true,
  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
