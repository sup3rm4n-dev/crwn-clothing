{
  "name": "crwn-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "12.4.0",
    "npm": "6.11.3"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "netlify-postbuild": "cd client && yarn install && yarn build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "stripe": "^7.9.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
  }
}
